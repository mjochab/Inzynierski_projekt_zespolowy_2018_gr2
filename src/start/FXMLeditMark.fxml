<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="start.FXMLeditMarkController">
   <children>
      <ImageView fitHeight="410.0" fitWidth="659.0" pickOnBounds="true">
         <image>
            <Image url="@tlo.jpg" />
         </image>
      </ImageView>
      <ComboBox fx:id="cbStudent" layoutX="49.0" layoutY="123.0" prefHeight="25.0" prefWidth="212.0" promptText="Student" />
      <Label layoutX="302.0" layoutY="32.0" text="Edycja Ocen" />
      <Button fx:id="prevBtn" layoutX="30.0" layoutY="363.0" mnemonicParsing="false" onAction="#prevWindow" prefHeight="25.0" prefWidth="65.0" text="Wróć" />
      <Button fx:id="Logout" layoutX="578.0" layoutY="20.0" mnemonicParsing="false" onAction="#logout" text="Wyloguj" />
      <Label layoutX="451.0" layoutY="81.0" prefHeight="17.0" prefWidth="44.0" text="Oceny" textAlignment="CENTER" />
      <Button fx:id="changeMark" layoutX="49.0" layoutY="270.0" mnemonicParsing="false" onAction="#redirectToChangeMark" prefHeight="59.0" prefWidth="92.0" text="Edytuj &#10;zaznaczoną &#10;ocene" textAlignment="CENTER" />
      <Button fx:id="addMark" layoutX="180.0" layoutY="270.0" mnemonicParsing="false" onAction="#redirectToAddNewMark" prefHeight="59.0" prefWidth="92.0" text="Dodaj ocenę" />
      <TableView fx:id="tablee" layoutX="394.0" layoutY="123.0" prefHeight="208.0" prefWidth="226.0">
        <columns>
          <TableColumn fx:id="col_nr_indeksu" prefWidth="75.0" text="Nr indeksu" />
          <TableColumn fx:id="col_nazwa" prefWidth="75.0" text="Przedmiot" />
            <TableColumn fx:id="col_ocena" prefWidth="75.0" text="Ocena" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
