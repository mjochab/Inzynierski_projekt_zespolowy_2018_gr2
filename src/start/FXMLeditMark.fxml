<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="start.FXMLeditMarkController">
   <children>
      <ImageView fitHeight="410.0" fitWidth="659.0" pickOnBounds="true">
         <image>
            <Image url="@tlo.jpg" />
         </image>
      </ImageView>
      <Button fx:id="prevBtn" layoutX="30.0" layoutY="363.0" mnemonicParsing="false" onAction="#prevWindow" prefHeight="25.0" prefWidth="65.0" text="Wróć" />
      <Button fx:id="Logout" layoutX="578.0" layoutY="20.0" mnemonicParsing="false" onAction="#logout" text="Wyloguj" />
      <Button fx:id="addMark" layoutX="476.0" layoutY="157.0" mnemonicParsing="false" onAction="#redirectToAddNewMark" prefHeight="59.0" prefWidth="92.0" text="Dodaj ocenę" />
      <TableView fx:id="tablee" layoutX="30.0" layoutY="82.0" prefHeight="208.0" prefWidth="374.0">
        <columns>
          <TableColumn fx:id="col_nr_indeksu" prefWidth="117.0" text="Nr indeksu" />
          <TableColumn fx:id="col_nazwa" prefWidth="151.0" text="Przedmiot" />
            <TableColumn fx:id="col_ocena" minWidth="0.0" prefWidth="105.0" text="Ocena" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
