<AnchorPane id="AnchorPane" fx:id="mainPane" prefHeight="362.0" prefWidth="578.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="start.ScholarshipController">
    <children>
        <ImageView blendMode="MULTIPLY" fitHeight="410.0" fitWidth="659.0" pickOnBounds="true">
            <image>
                <Image url="@tlo.jpg" />
            </image>
        </ImageView>
        <Label contentDisplay="CENTER" layoutX="301.0" layoutY="14.0" prefHeight="17.0" prefWidth="58.0" text="Stypendia" textAlignment="CENTER" />
        <Button fx:id="btnBackward" layoutX="286.0" layoutY="335.0" mnemonicParsing="false" onAction="#backToStudent" prefHeight="25.0" prefWidth="88.0" text="Wróć" textAlignment="CENTER" />
        <TextArea editable="false" layoutX="23.0" layoutY="294.0" prefHeight="32.0" prefWidth="499.0" promptText="Po złożeniu wniosku student ma obowiązek dostarczyć komplet dokumentów do dziekanatu maksymalnie w okresie 10 dni roboczych.">
            <font>
                <Font size="9.0" />
            </font>
        </TextArea>
        <CheckBox layoutX="531.0" layoutY="303.0" mnemonicParsing="false" prefHeight="15.0" prefWidth="88.0" text="Zgadzam się">
            <font>
                <Font size="10.0" />
            </font>
        </CheckBox>
