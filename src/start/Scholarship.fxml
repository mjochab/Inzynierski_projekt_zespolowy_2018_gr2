          <TableColumn fx:id="colnr_indeksu" prefWidth="75.0" text="nr_indeksu" />
          <TableColumn fx:id="col_srednia" prefWidth="75.0" text="srednia" />
        </columns>
      </TableView>
      <Label fx:id="indeks" layoutX="637.0" layoutY="387.0" prefHeight="0.0" prefWidth="0.0" text="Label" />
      <Button fx:id="zlozButton" layoutX="287.0" layoutY="205.0" mnemonicParsing="false" onAction="#zlozWniosek" text="Złóż wniosek" />
    
    </children>
</AnchorPane>
